    ".": {
      "types": "./ngx-tetris.d.ts",
      "esm2020": "./esm2020/ngx-tetris.mjs",
      "es2020": "./fesm2020/ngx-tetris.mjs",
      "es2015": "./fesm2015/ngx-tetris.mjs",
      "node": "./fesm2015/ngx-tetris.mjs",
      "default": "./fesm2020/ngx-tetris.mjs"
