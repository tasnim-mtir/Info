{
  "name": "ngx-tetris",
  "description": "Tetris game as an angular component, its only the core of the game... you need to add everything around it (controls, score...)  yourself :) ",
  "version": "1.1.0",
  "author": {
    "name": "Chrystian Ruminowicz",
    "email": "chrystianr@gmail.com"
  },
  "homepage": "http://chrum.it/pages/ngx-tetris",
  "keywords": [
    "angular",
    "ngx",
    "ng",
    "typescript",
    "tetris",
    "game",
    "core"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrum/ngx-tetris"
  },
  "peerDependencies": {
    "@angular/common": ">6.1.0",
    "@angular/core": ">6.1.0"
  },
  "module": "fesm2015/ngx-tetris.mjs",
  "es2020": "fesm2020/ngx-tetris.mjs",
  "esm2020": "esm2020/ngx-tetris.mjs",
  "fesm2020": "fesm2020/ngx-tetris.mjs",
  "fesm2015": "fesm2015/ngx-tetris.mjs",
  "typings": "ngx-tetris.d.ts",
  "exports": {
